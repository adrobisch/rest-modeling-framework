main(vendorName) ::= <<
{
  "name": "<vendorName; format="lowerHyphen">/raml-php-sdk",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "<vendorName>\\\\": [
        "src/"
      ]
    }
  },
  "require": {
    "php": ">=5.6",
    "guzzlehttp/psr7": "^1.1",
    "guzzlehttp/guzzle": "^6.0 || ^5.3.1",
    "psr/cache": "^1.0",
    "psr/log": "^1.0",
    "psr/container": "^1.0",
    "cache/filesystem-adapter": "^1.0"
  }
}

>>
