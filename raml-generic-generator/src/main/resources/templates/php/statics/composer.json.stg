main(vendorName) ::= <<
{
  "name": "<vendorName; format="lowerHyphen">/raml-php-sdk",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "<vendorName>\\\\": [
        "src/"
      ],
      "<vendorName>\\\\Test\\\\": [
        "test/unit/"
      ]
    }
  },
  "require": {
    "php": ">=7.1",
    "guzzlehttp/psr7": "^1.1",
    "guzzlehttp/guzzle": "^6.0 || ^5.3.1",
    "psr/cache": "^1.0",
    "psr/log": "^1.0",
    "cache/filesystem-adapter": "^1.0"
  },
  "require-dev": {
    "monolog/monolog": "^1.3",
    "phpunit/phpunit": "^6.0"
  }
}

>>
